    "isbot": "^5.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@atproto/lex-cli": "0.3.2",
    "@mkizka/eslint-config": "^4.2.1",
    "@remix-run/dev": "^2.9.1",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
