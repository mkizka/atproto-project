{
  "name": "remix-project2",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "remix vite:build",
    "dev": "tsx ./app/server.js",
    "format": "prettier . --write && eslint . --fix",
    "postinstall": "tsx ./scripts/generate.ts",
    "prepare": "husky",
    "start": "NODE_ENV=production tsx ./app/server.ts",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@atproto/lexicon": "^0.4.0",
    "@atproto/xrpc": "^0.5.0",
    "@atproto/xrpc-server": "^0.5.1",
    "@remix-run/express": "^2.9.1",
    "@remix-run/node": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "express": "^4.19.2",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@atproto/lex-cli": "0.3.2",
    "@mkizka/eslint-config": "^4.2.1",
    "@remix-run/dev": "^2.9.1",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.38.0",
    "glob": "^10.3.12",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-pkg": "^0.18.1",
    "tsx": "^4.9.3",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "sideEffects": false,
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{js,mjs,ts,tsx}": "eslint --fix"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-pkg"
    ]
  }
}
