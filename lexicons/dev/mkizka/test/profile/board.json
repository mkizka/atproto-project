{
  "lexicon": 1,
  "id": "dev.mkizka.test.profile.board",
  "defs": {
    "main": {
      "type": "record",
      "description": "Record containing a cards of your profile.",
      "key": "literal:self",
      "record": {
        "type": "object",
        "required": ["cards", "createdAt"],
        "properties": {
          "cards": {
            "type": "array",
            "description": "List of cards in the board.",
            "items": {
              "type": "union",
              "refs": ["dev.mkizka.test.profile.defs#blueskyCard"]
            }
          },
          "createdAt": {
            "type": "string",
            "description": "Creation date of the board."
          }
        }
      }
    }
  }
}
